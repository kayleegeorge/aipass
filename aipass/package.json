    "crypto-js": "^4.2.0",
    "framer-motion": "^10.16.12",
    "next": "14.0.2",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
